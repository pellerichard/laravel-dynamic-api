{
  "name": "pellerichard/laravel-dynamic-api",
  "description": "Laravel Dynamic API",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/pellerichard/laravel-dynamic-api/issues",
    "source": "https://github.com/pellerichard/laravel-dynamic-api"
  },
  "keywords": [
    "laravel-dynamic-api"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Pelle RichÃ¡rd",
      "email": "whadez@icloud.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pellerichard\\LaravelDynamicApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pellerichard\\LaravelDynamicApi\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pellerichard\\LaravelDynamicApi\\DynamicApiServiceProvider"
      ],
      "aliases": {
        "ApiService": "Pellerichard\\LaravelDynamicApi\\Services\\Facades\\ApiService"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
